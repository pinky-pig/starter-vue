{
  "name": "playground",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3333 --open --host",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "build": "vite build",
    "build:lib": "vite build --mode lib && npm run build:types",
    "build:types": "vue-tsc -p tsconfig.build.json && api-extractor run && rimraf temp",
    "release:lib": "bumpp -c \"release: v%s\" packages/package.json && tsx scripts/copy.ts && cd output-lib && npm publish",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "up": "taze major -I",
    "copy": "tsx scripts/copy.ts",
    "zip": "tsx scripts/export-zip.ts",
    "fetch-vsc-settings": "tsx scripts/vsc-settings.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@arvin/maps": "workspace:^",
    "@arvin/materials": "workspace:^",
    "@arvin/utils": "workspace:^",
    "@types/cesium": "^1.70.0",
    "cesium": "^1.118.2",
    "pinia": "^2.1.7",
    "vite-plugin-cesium": "^1.2.22"
  },
  "devDependencies": {
    "@arvinn/eslint-config": "^0.0.4",
    "@arvinn/prettier-config": "^0.0.1",
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@iconify-json/carbon": "^1.1.35",
    "@iconify-json/lucide": "^1.1.191",
    "@iconify/json": "^2.2.218",
    "@microsoft/api-extractor": "^7.47.0",
    "@types/archiver": "^6.0.2",
    "@types/clean-css": "^4.2.11",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^10.10.0",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "bumpp": "^9.4.1",
    "class-variance-authority": "^0.7.0",
    "clean-css": "^5.3.3",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.4.0",
    "fs": "0.0.1-security",
    "https-localhost": "^4.7.1",
    "jsdom": "^24.1.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.5",
    "lucide-vue-next": "^0.394.0",
    "pnpm": "^9.3.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.1",
    "radix-vue": "^1.8.3",
    "rimraf": "^5.0.7",
    "shadcn-vue": "^0.10.5",
    "shiki": "^1.6.3",
    "simple-git": "^3.25.0",
    "simple-git-hooks": "^2.11.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "taze": "^0.13.8",
    "tsx": "^4.15.2",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "unplugin-vue-router": "^0.9.1",
    "vite": "^5.2.13",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-ssg": "^0.23.7",
    "vite-ssg-sitemap": "^0.7.1",
    "vitepress": "^1.2.3",
    "vitest": "^1.6.0",
    "vue": "^3.4.27",
    "vue-router": "^4.3.3",
    "vue-sonner": "^1.1.2",
    "vue-tsc": "^2.0.21"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "prettier": "@arvinn/prettier-config"
}
